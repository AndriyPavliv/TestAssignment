<table class="table table-condensed table-bordered table-striped-rows">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Title</th>
            <th>Products Sold</th>
            <th>Refers To</th>            
        </tr>
    </thead>
    <tbody data-bind="foreach: EmployeeSalesFiltered">
        <tr>
            <td data-bind="text: FirstName"></td>
            <td data-bind="text: LastName"></td>
            <td data-bind="text: Title"></td>
            <td data-bind="text: NumberOfProductsSold"></td>
            <td data-bind="text: RefersTo"></td>            
        </tr>
    </tbody>
</table>